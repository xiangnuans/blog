{
  "title": "book",
  "author": "guoyanhong",
  "links": {
    "sidebar": {
      "Home": "https://coco723.github.io/blog"
    }
  },
  "plugins": [
    "-lunr",
    "-search",
    "search-pro",
    "-sharing",
    "disqus",
    "github-buttons",
    "editlink",
    "anchor-navigation-ex",
    "page-footer-ex",
    "splitter",
    "copy-code-button",
    "expandable-chapters-small",
    "donate",
    "-highlight",
    "prism"
  ],
  "pluginsConfig": {
    "search-pro": {
      "cutWordLib": "nodejieba",
      "defineWord": [
        "Gitbook Use"
      ]
    },
    "anchor-navigation-ex": {
      "showLevel": false
    },
    "disqus": {
      "shortName": "book"
    },
    "editlink": {
      "base": "https://github.com/coco723/blog/blob/master",
      "label": "Edit",
      "multilingual": false
    },
    "github-buttons": {
      "buttons": [
        {
          "user": "coco723",
          "repo": "book",
          "type": "star",
          "size": "small",
          "count": true
        }
      ]
    },
    "prism": {
      "ignore": [
        "mermaid",
        "eval-js"
      ],
      "lang": {
        "shell": "bash",
        "ts": "javascript"
      },
      "css": [
        "prismjs/themes/prism-solarizedlight.css"
      ]
    },
    "page-footer-ex": {
      "copyright": "By [guoyanhong](https://github.com/coco723)，使用[知识共享 署名-相同方式共享 4.0协议](https://creativecommons.org/licenses/by-sa/4.0/)发布",
      "markdown": true,
      "update_label": "此页面修订于：",
      "update_format": "YYYY-MM-DD HH:mm:ss"
    },
    "donate": {
      "wechat": "https://tva1.sinaimg.cn/large/008i3skNly1gpybxpywl9j30u014q761.jpg",
      "alipay": "https://tva1.sinaimg.cn/large/008i3skNly1gpybxpywl9j30u014q761.jpg",
      "title": "",
      "button": "赏",
      "alipayText": "支付宝打赏",
      "wechatText": "微信打赏"
    }
  },
  "ignores": [
    "_book",
    "node_modules"
  ]
}